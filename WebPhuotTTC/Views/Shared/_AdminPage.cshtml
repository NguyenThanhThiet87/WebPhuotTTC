@{
    ViewBag.Title = "AdminPage";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/AdminPage/AdminPage.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- jQuery (Load first) -->
    <script src="~/Scripts/jquery-3.7.1.min.js"></script>

    <!-- jQuery Unobtrusive AJAX -->
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    <!-- Bootstrap 5 (Optional) -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <!-- Select2 Library-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/css/select2.min.css" integrity="sha512-nMNlpuaDPrqlEls3IX/Q56H36qvBASwb3ipuo3MxeWbsQB1881ox0cRv7UPTgBlriqoynt35KjEwgGUeUXIPnw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/js/select2.min.js" integrity="sha512-2ImtlRlf2VVmiGZsjm9bEyhjGW4dU7B6TNwh/hx/iSByxNENtj3WVE6o/9Lj4TJeVXPi4bnOIMXFIJJAeufa0A==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <!-- Chartjs Library-->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body>
    <div class="admin_header">
        <div class="admin_header-logo">
            <img src="https://res.cloudinary.com/duouljna1/image/upload/v1727272425/Gold_Red_Retro_Vintage_Motorcycle_Club_Badge_Logo_uc4wti.png" alt="error" />
            <h2>TTC - ADMIN</h2>
        </div>
        <ul class="admin_header-nav">
            <li>
                <i class="fa-solid fa-bell" style="color: #ffffff;"></i>
            </li>
            <li>
                @Html.Action("ItemAccountInHeadAdmin", "Account")
            </li>
        </ul>
    </div>
    <div class="admin_content">
        <ul class="admin_content-left">
            <li class="menu_item">
                <a href="/Admin/Home">
                    <div>
                        <i class="fa-solid fa-house" style="color: #ffffff;"></i>
                        <span>Trang chủ</span>
                    </div>
                </a>
            </li>
            <li class="menu_item item_manage" onclick="ShowDownListItem()">
                <a>
                    <div>
                        <i class="fa-solid fa-list-check"></i>
                        <span>Quản lý</span>
                    </div>
                    <i class="fa-solid fa-chevron-right"></i>
                </a>
            </li>
            <li class="menu_item submenu_item hide_element ">
                <a href="/Admin/Sanpham">
                    <div>
                        <i class="fa-solid fa-boxes-stacked" style="color: #ffffff;"></i>
                        <span>Sản phẩm</span>
                    </div>
                </a>
            </li>
            <li class="menu_item submenu_item hide_element ">
                <a href="/Admin/Nhomsanpham">
                    <div>
                        <i class="fa-solid fa-layer-group" style="color: #ffffff;"></i>
                        <span>Nhóm sản phẩm</span>
                    </div>
                </a>
            </li>
            <li class="menu_item submenu_item hide_element ">
                <a href="/Admin/Loaisanpham">
                    <div>
                        <i class="fa-solid fa-layer-group" style="color: #ffffff;"></i>
                        <span>Loại sản phẩm</span>
                    </div>
                </a>
            </li>
            <li class="menu_item submenu_item hide_element">
                <a href="/Admin/Nguoidung">
                    <div>
                        <i class="fa-solid fa-tags" style="color: #ffffff;"></i>
                        <span>Người dùng</span>
                    </div>
                </a>
            </li>
            <li class="menu_item submenu_item hide_element">
                <a href="/Admin/Thuonghieu">
                    <div>
                        <i class="fa-solid fa-certificate" style="color: #ffffff;"></i>
                        <span>Thương hiệu</span>
                    </div>
                </a>
            </li>
            <li class="menu_item">
                <a href="/Admin/Vourcher">
                    <div>
                        <i class="fa-solid fa-users" style="color: #ffffff;"></i>
                        <span>Khuyến mãi</span>
                    </div>
                </a>
            </li>
            <li class="menu_item">
                <a href="/Admin/DonHang">
                    <div>
                        <i class="fa-solid fa-pen-to-square" style="color: #ffffff;"></i>
                        <span>Đơn Đặt Hàng</span>
                    </div>
                </a>
            </li>
        </ul>
        <div class="admin_content-right">
            @RenderBody()
        </div>
    </div>
    <!-- Custom JS-->
    <script src="~/Scripts/AdminPage/JavaScript.js"></script>
</body>
</html>